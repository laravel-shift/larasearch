{
    "name": "gtk/larasearch",
    "description": "A driver based solution to searching your Eloquent models supports Laravel 5.2 and Elasticsearch engine.",
    "keywords": [
        "algolia",
        "elasticsearch",
        "laravel",
        "search"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Giang Thai Cuong",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/bus": "^6.0",
        "illuminate/contracts": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/pagination": "^6.0",
        "illuminate/queue": "^6.0",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "algolia/algoliasearch-client-php": "^1.10",
        "elasticsearch/elasticsearch": "~2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Gtk\\Larasearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "algolia/algoliasearch-client-php": "Required to use the Algolia engine.",
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
